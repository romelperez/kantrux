{
  "extends": [
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "browser": true
  },

  "plugins": [
    "prettier",
    "flowtype",
    "react"
  ]
}
