{
  "name": "kantrux",
  "version": "1.0.0-alpha.8",
  "description": "Ultralight web UI library for building static components with JSX support",
  "keywords": [
    "ui",
    "library",
    "jsx",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romelperez/kantrux.git"
  },
  "homepage": "https://github.com/romelperez/kantrux",
  "author": "Romel PÃ©rez (romelperez.com)",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "rimraf ./dist && babel ./src --out-dir ./dist --ignore \"./src/**/*.test.js\"",
    "lint": "eslint ./src",
    "test": "jest",
    "test-dev": "jest --watch",
    "test-ci": "npm run compile && npm run lint && npm run test",
    "changelog": "auto-changelog --output CHANGELOG.md --commit-limit false",
    "release": "npm run test-ci && npm publish",
    "contributors-add": "all-contributors add",
    "contributors-generate": "all-contributors generate"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "all-contributors-cli": "^6.6.1",
    "auto-changelog": "^1.13.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "rimraf": "^2.6.3"
  }
}
