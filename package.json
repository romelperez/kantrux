{
  "name": "kantrux",
  "version": "1.0.0-alpha.3",
  "description": "Simple web UI library for building static components in JSX",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "format": "prettier --write \"./src/**/*.js\"",
    "test": "jest",
    "test-dev": "jest --watch",
    "precommit": "npm test",
    "compile": "rimraf ./dist && babel ./src --out-dir ./dist --ignore \"./src/**/*.test.js\"",
    "compile-copyfiles": "cp package.json dist/package.json && cp README.md dist/README.md",
    "changelog": "auto-changelog --output CHANGELOG.md --commit-limit false",
    "release": "npm test && npm run compile && npm run compile-copyfiles && npm publish dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "auto-changelog": "^1.10.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.2"
  }
}
